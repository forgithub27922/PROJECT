<odoo>
    <record id="emp_time_tracking_check" model="ir.cron">
        <field name="name">Employee Time Tracking Check For Penalties</field>
        <field name="model_id" ref="model_time_tracking"/>
        <field name="state">code</field>
        <field name="code">model._check_time_tracking()</field>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="doall" eval="True"/>
    </record>
</odoo>