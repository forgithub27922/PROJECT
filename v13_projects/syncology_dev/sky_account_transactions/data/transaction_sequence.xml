<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequence for Transaction -->
        <record id="transaction_sequence" model="ir.sequence">
            <field name="name">Transaction Sequence</field>
            <field name="code">account.transaction</field>
            <field name="prefix">TXN/%(range_year)s/</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>
    </data>
</odoo>