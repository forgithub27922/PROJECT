#. module: sky_hr_recruitment_custom
#: model:mail.template,body_html:sky_hr_recruitment_custom.accept_applicant_email_template
msgid ""
"<div>\n"
"                    <p>\"Congratulations. You have passed your interview for the position <b>${object.job_id.name}.</b>\n"
"                        <br/><br/>\n"
"                        Please expect an email from us in the coming few days for instructions on what to do next, and the documents required from your side.\n"
"                        <br/><br/>\n"
"                        Thanks and see you on board.\"\n"
"                    </p>\n"
"                </div>\n"
"            "
msgstr ""

#. module: sky_hr_recruitment_custom
#: model:mail.template,body_html:sky_hr_recruitment_custom.cancel_applicant_email_template
#: model:mail.template,body_html:sky_hr_recruitment_custom.reject_applicant_email_template
msgid ""
"<div>\n"
"                    <p>\"We are very sorry to inform you that your application to can no longer be processed, due to :\n"
"                        <b>${object.reason}.</b>\n"
"                        <br/>\n"
"                        We wish you the best in your future endeavors.\"\n"
"                    </p>\n"
"                </div>\n"
"            "
msgstr ""

#. module: sky_hr_recruitment_custom
#: model:mail.template,body_html:sky_hr_recruitment_custom.schedule_interview_email_template
msgid ""
"<div>\n"
"                    <p>\"Your application to has been moved to the next stage, which is the interview.\n"
"                    <br/>\n"
"                    <br/>\n"
"                    Date : ${object.interview_date}\n"
"                    <br/>\n"
"                    <br/>\n"
"                    Time : ${object.interview_time}\n"
"                    <br/>\n"
"                    <br/>\n"
"                    Location : ${object.interview_location}\n"
"                    <br/>\n"
"                    <br/>\n"
"                    Please attend at least 10 minutes before the provided time.\"\n"
"                    </p>\n"
"                </div>\n"
"            "
msgstr ""

#. module: sky_hr_recruitment_custom
#: model:mail.template,body_html:sky_hr_recruitment_custom.pending_applicant_document_email_template
msgid ""
"<div>\n"
"                    <p>You have been selected as <b>${object.job_id.name}</b> at\n"
"                        <b>${object.company_id.name}</b>\n"
"                    </p>\n"
"\n"
"                    <p>Kindly submit the documents before joining.</p>\n"
"                    <p>We look forward to your on boarding.</p>\n"
"                    <p>Thanks and Regards,</p>\n"
"                </div>\n"
"\n"
"            "
msgstr ""

