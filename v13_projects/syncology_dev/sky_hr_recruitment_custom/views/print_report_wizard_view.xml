<odoo>

    <!-- From view of the Reject Applicant  Wizard -->
    <record id="view_print_report_wizard_form" model="ir.ui.view">
        <field name="name">view.print.report.wizard.form</field>
        <field name="model">print.report.wizard</field>
        <field name="arch" type="xml">
             <form>
                 <sheet>
                     <group>
                         <field name="download_file" filename="fname"/>
                         <field name="fname" invisible="1"/>
                     </group>
                 </sheet>
                 <footer>
                     <button special="cancel" string="Cancel"/>
                 </footer>
             </form>
        </field>
    </record>

</odoo>