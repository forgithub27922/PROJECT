"                % elif object.partner_id.name and object.partner_id.title.name in ['Herr','Herr Dr.','Herr Prof.','Dr.','Dipl. Ing.']:\n"
"                  <p style=\"margin:0px 0px 9px 0px;font-size:13px;font-family:&quot;Lucida Grande&quot;, Helvetica, Verdana, Arial, sans-serif;\">Sehr geehrter ${object.partner_id.title.name} ${object.partner_id.name},</p>\n"
"                % else:\n"
"                  <p style=\"margin:0px 0px 9px 0px;font-size:13px;font-family:&quot;Lucida Grande&quot;, Helvetica, Verdana, Arial, sans-serif;\">Sehr geehrte Damen und Herren,</p>\n"
"                % endif\n"
"                % if object.state in ('draft', 'sent'):\n"
"                  <p style=\"margin:0px 0px 9px 0px;font-size:13px;font-family:&quot;Lucida Grande&quot;, Helvetica, Verdana, Arial, sans-serif;\"><span style=\"line-height: 14.95px\">nachfolgend erhalten Sie das angefragte Angebot. Wir freuen uns auf eine Zusammenarbeit.</span></p>\n"
