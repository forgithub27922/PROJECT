<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="grimm_product_attribute_form_view_inherit" model="ir.ui.view">
            <field name="name">grimm.product.attribute.set.form.view.inherit</field>
            <field name="model">product.attribute</field>
            <field name="inherit_id" ref="grimm_product.of_product_attribute_form_view"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='name']" position="after">
                    <field name="print_attrib"/>
                    <field name="print_seq"/>
                </xpath>
            </field>
        </record>

        <record id="grimm_product_attribute_tree_view_inherit" model="ir.ui.view">
            <field name="name">grimm.product.attribute.set.tree.view.inherit</field>
            <field name="model">product.attribute</field>
            <field name="inherit_id" ref="grimm_product.of_product_attribute_tree_view"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='name']" position="after">
                    <field name="technical_name"/>
                    <field name="print_attrib"/>
                    <field name="print_seq"/>
                </xpath>
            </field>
        </record>

