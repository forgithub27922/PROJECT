<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="view_update_age_wiz_form" model="ir.ui.view">
        <field name="name">view.update.age.wiz.form</field>
        <field name="model">update.age.wizard</field>
        <field name="inherit_id" ref="hotel_mangement_14.view_update_fee_wiz_form_customer"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='age']" position="after">
                <field name="mob_no"/>
                <field name="barcode"/>
            </xpath>

        </field>
    </record>


</odoo>